@{
    ViewData["Title"] = "";
}

@section Styles{
    <link href="~/css/home.css" rel="stylesheet" />
    <script type="text/javascript" src="~/js/site.js"></script>
    <link href="common-css/ionicons.css" rel="stylesheet">
    <link rel="stylesheet" href="common-css/jquery.classycountdown.css" />
    <link href="common-css/bootstrap.css" rel="stylesheet">
    <link href="common-css/styles.css" rel="stylesheet">
    <link href="common-css/responsive.css" rel="stylesheet">
}

<div class="w3-sidebar w3-bar-block" style="display:none" id="mySidebar">
    <button onclick="w3_close()" class="w3-bar-item w3-large">&times;</button>
    <a href="#home" class="w3-bar-item w3-button">Home</a>
    <a href="#gallery" class="w3-bar-item w3-button">Gallery</a>
    <a href="#wedding" class="w3-bar-item w3-button">Wedding Details</a>
    <a href="#accommodation" class="w3-bar-item w3-button">Accommodation</a>
    <a href="#travel" class="w3-bar-item w3-button">Travel</a>
    <a href="#guestbook" class="w3-bar-item w3-button">Guest Book</a>
</div>

<div class="menu-container d-flex w3-center">
    <button class="w3-button w3-xlarge" onclick="w3_open()">☰</button>
</div>

<div class="mainpic-container">
    <nav class="mainpic-nav">
        <div class="background-img"></div>
    </nav>
    <div class="name-text">
        <h1 class="name cursive">Sunny</h1>
        <h1 class="name cursive">&amp; Komal</h1>
    </div>
</div>

<div class="w3-main" style="margin-left:55%">
    <div id="us" class="sunnykomal-container">
        <div>
            <h1 class="goa-text cursive">Goa, India</h1><h3 class="date-text cursive">02.02.2020</h3>
            <div id="rounded-countdown">
                <div class="countdown"></div>
            </div>
        </div>
    </div>

    <div id="gallery" class="gallery-text cursive">Gallery</div>

    <div class="gallery-container col-lg-12 col-sm-12">
        <div class="col-lg-12 col-sm-12">
            <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
                <div class="carousel-inner">
                    <div class="carousel-item active">
                        <img class="d-block w-100" src="~/images/gallery1.jpg">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100" src="~/images/gallery2.jpg">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100" src="~/images/gallery3.jpg">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100" src="~/images/gallery4.jpg">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100" src="~/images/gallery5.jpg">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100" src="~/images/gallery6.jpg">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100" src="~/images/gallery7.jpg">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100" src="~/images/gallery8.jpg">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100" src="~/images/gallery9.jpg">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100" src="~/images/gallery10.jpg">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100" src="~/images/gallery11.jpg">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100" src="~/images/gallery12.jpg">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100" src="~/images/gallery13.jpg">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100" src="~/images/gallery14.jpg">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100" src="~/images/gallery15.jpg">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100" src="~/images/gallery16.jpg">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100" src="~/images/gallery17.jpg">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100" src="~/images/gallery18.jpg">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100" src="~/images/gallery19.jpg">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100" src="~/images/gallery20.jpg">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100" src="~/images/gallery21.jpg">
                    </div>
                </div>
            </div>
            <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
            </a>
        </div>
    </div>
    <hr />
    <div id="wedding" class="wedding-container">
        <div class="header-text cursive">Wedding Details</div>
        <div class="timeline-container">
            <div>
                <div class="date-container"><span class="timeline-date">February 1<span class="ordinal">st</span>, 2020</span></div>
                <div class="timeline">
                    <ul>
                        <li>
                            <div class="content">
                                <img src="~/images/arrival.png" alt="arrival" />
                            </div>
                            <div class="time">
                                <h4>2:00 PM <br /></h4>
                                <h4>Check-in</h4>
                            </div>
                        </li>

                        <li>
                            <div class="content">
                                <img src="~/images/sangeet.jpg" alt="sangeet" />
                            </div>
                            <div class="time">
                                <h4>6:00 PM Onwards<br /></h4>
                                <h4>Sangeet Sandhya &amp; Mehndi</h4>
                            </div>
                        </li>

                        <div style="clear:both;"></div>
                    </ul>
                </div>
            </div>
            <div>
                <div class="date-container"><span class="timeline-date">February 2<span class="ordinal">nd</span>, 2020</span></div>
                <div class="timeline">
                    <ul>
                        <li>
                            <div class="content">
                                <img src="~/images/haldi.png" alt="haldi" />
                            </div>
                            <div class="time">
                                <h4>10:00 AM <br /></h4>
                                <h4>Pithi &amp; Mandap Muhrat</h4>
                            </div>
                        </li>
                        <li>
                            <div class="content">
                                <img src="~/images/wedding.png" alt="wedding" />
                            </div>
                            <div class="time">
                                <h4>5:00 PM Onwards<br /></h4>
                                <h4>Baraat &amp; Wedding</h4>
                                <h6>(Dress Code: Indian Traditional)</h6>
                            </div>
                        </li>

                        <div style="clear:both;"></div>
                    </ul>
                </div>
            </div>
            <div>
                <div class="date-container"><span class="timeline-date">February 3<span class="ordinal">rd</span>, 2020</span></div>
                <div class="timeline">
                    <ul>
                        <li>
                            <div class="content">
                                <img src="~/images/departure.png" alt="departure" />
                            </div>
                            <div class="time">
                                <h4>11:00 AM <br /></h4>
                                <h4>Check-out</h4>
                            </div>
                        </li>
                        <div style="clear:both;"></div>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <hr />
    <div id="accommodation" class="accommodation-container">
        <div class="header-text cursive">Accommodation</div>
        <h3>Holiday Inn Resort</h3>
        <img src="~/images/holidayinn.jpg" alt="holidayinn" />
        <div class="accommodation-details">
            <p><h4>Mobor Beach, Cavelossim, Goa, India</h4></p>
            <p>
                For your comfort and convenience, it is our pleasure to have arrangements made
                at Holiday Inn Resort, Goa for your stay for February 1st and 2nd, 2020 (per your RSVP).
            </p>
            <p>
                Transportation to &amp; from the resort will be provided for Goa International Airport and Madgaon Railway
                Station. To ensure proper planning, we kindly request your travel itineraries by December
            </p>
        </div>
    </div>
    <hr />
    <div id="travel" class="travel-container">
        <div class="header-text cursive">Travel</div>
        <div class="container">
            <p><h4>Mumbai to Goa</h4></p>
            <p>

            </p>
            <p><h4>Surat to Goa</h4></p>
            <p>

            </p>
        </div>
    </div>
    

    <div id="guestbook" class="guestbook-container">
        <div>
        </div>
    </div>
</div>


    @section Scripts{
        <script type="text/javascript" src="common-js/jquery-3.1.1.min.js"></script>
        <script type="text/javascript" src="common-js/tether.min.js"></script>
        <script type="text/javascript" src="common-js/bootstrap.js"></script>
        <script type="text/javascript" src="common-js/jquery.classycountdown.js"></script>
        <script type="text/javascript" src="common-js/jquery.knob.js"></script>
        <script type="text/javascript" src="common-js/jquery.throttle.js"></script>
        <script type="text/javascript" src="common-js/scripts.js"></script>
        <script src="~/js/site.js"></script>
    }
